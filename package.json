{
  "name": "ronaldjdev.github.io",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "indexer": "node node_modules/react-component-indexer/src/export.js",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|scss|ts|tsx)\"",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|css|scss|ts|tsx|md)\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.3.1",
    "gh-pages": "^6.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-component-indexer": "^1.0.6",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.20.0",
    "react-svg": "^16.1.31",
    "react-tooltip": "^5.24.0",
    "sass": "^1.69.5",
    "swiper": "^11.0.5",
    "tailwindcss": "^3.3.5",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
